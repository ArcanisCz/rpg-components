{
  "name": "@morosystems/sparc_ui",
  "version": "0.0.1",
  "description": "UI for SPARC",
  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm run clean-dist && npm run build:html && npm run lint && npm run test",
    "build": "babel-node tools/build.js",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter spec",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:watch": "npm run test -- --watch"
  },
  "author": "Morosystems",
  "license": "Ask info@morosystems.cz",
  "private": true,
  "dependencies": {
    "bootstrap": "3.3.7",
    "icheck": "1.0.2",
    "immutable": "3.8.1",
    "moment": "2.14.1",
    "object-assign": "4.1.0",
    "object-resolve-path": "1.1.1",
    "react": "15.1.0",
    "react-bootstrap": "0.30.2",
    "react-dom": "15.1.0",
    "react-helmet": "3.1.0",
    "react-icheck": "0.3.4",
    "react-overlays": "0.6.6",
    "react-loading-spinner": "1.0.12",
    "react-redux": "4.4.5",
    "react-router": "2.6.1",
    "react-select": "1.0.0-beta14",
    "react-tabs-redux": "1.3.0",
    "react-widgets": "4.0.0-beta.3",
    "redux": "3.5.2",
    "redux-form": "5.3.2",
    "redux-immutable": "3.0.6",
    "redux-saga": "0.11.0",
    "redux-thunk": "2.1.0",
    "uuid": "2.0.2",
    "whatwg-fetch": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-polyfill": "6.13.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "6.11.6",
    "browser-sync": "2.14.0",
    "chai": "3.5.0",
    "chai-immutable": "1.6.0",
    "chalk": "1.1.3",
    "cheerio": "0.20.0",
    "connect-history-api-fallback": "1.2.0",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.2.2",
    "eslint-config-airbnb": "10.0.0",
    "eslint-plugin-import": "1.12.0",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "6.0.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "isparta": "4.0.0",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mocha": "3.0.1",
    "npm-run-all": "2.3.0",
    "react-addons-test-utils": "15.1.0",
    "react-hot-loader": "1.3.0",
    "react-toggle": "2.1.1",
    "redux-immutable-state-invariant": "1.2.3",
    "rimraf": "2.5.4",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-version-file-plugin": "0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "https://git.morosystems.cz/projects/EBAY/repos/sparcui"
  }
}
