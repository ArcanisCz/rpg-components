{
    "name": "moro-components",
    "version": "0.0.1",
    "description": "Moro compoents",
    "engines": {
        "npm": ">=3"
    },
    "scripts": {
        "preinstall": "node tools/nodeVersionCheck.js",
        "setup": "node tools/setup/setupMessage.js && npm install && node tools/setup/setup.js",
        "start-message": "babel-node tools/startMessage.js",
        "prestart": "npm-run-all --parallel start-message remove-dist",
        "start": "npm-run-all --parallel open:src lint:watch",
        "open:src": "babel-node tools/srcServer.js",
        "lint": "esw webpack.config.* src tools --color --fix",
        "lint:watch": "npm run lint -- --watch",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "prebuild": "npm run clean-dist && npm run lint && npm run test",
        "build": "babel-node tools/build.js",
        "test": "mocha tools/testSetup.js \"src/components/**/*.spec.js\" --reporter progress",
        "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/components/**/*.spec.js\" --reporterprogress",
        "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha-- --require ./tools/testSetup.js \"src/components/**/*.spec.js\" && cat ./coverage/lcov.info |node_modules/coveralls/bin/coveralls.js",
        "test:watch": "npm run test -- --watch",
        "open:cover": "npm run test:cover && open coverage/index.html"
    },
    "author": "Morosystems",
    "license": "Ask Morosystems",
    "dependencies": {
        "react": "15.3.2",
        "react-dom": "15.3.2",
        "react-router": "2.8.1"
    },
    "devDependencies": {
        "autoprefixer": "6.5.0",
        "babel-cli": "6.16.0",
        "babel-core": "6.16.0",
        "babel-loader": "6.2.5",
        "babel-preset-moro": "0.3.1",
        "babel-preset-react-hmre": "1.1.1",
        "babel-register": "6.16.3",
        "chai": "3.5.0",
        "chalk": "1.1.3",
        "connect-history-api-fallback": "1.3.0",
        "cross-env": "3.0.0",
        "css-loader": "0.25.0",
        "enzyme": "2.4.1",
        "eslint": "3.7.0",
        "eslint-config-moro": "0.2.0-5",
        "eslint-plugin-import": "2.0.0",
        "eslint-watch": "2.1.14",
        "extract-text-webpack-plugin": "1.0.1",
        "file-loader": "0.9.0",
        "html-webpack-plugin": "2.22.0",
        "isparta": "4.0.0",
        "less": "2.7.1",
        "less-loader": "2.2.3",
        "mocha": "3.1.0",
        "mockdate": "1.0.4",
        "npm-run-all": "3.1.0",
        "open": "0.0.5",
        "postcss-loader": "0.13.0",
        "prompt": "1.0.0",
        "react-addons-test-utils": "15.3.2",
        "react-hot-loader": "3.0.0-beta.5",
        "redux-immutable-state-invariant": "1.2.3",
        "replace": "0.3.0",
        "rimraf": "2.5.4",
        "sinon": "1.17.6",
        "sinon-chai": "2.8.0",
        "style-loader": "0.13.1",
        "url-loader": "0.5.7",
        "webpack": "1.13.2",
        "webpack-dev-server": "1.16.1",
        "webpack-md5-hash": "0.0.5"
    },
    "repository": {
        "type": "git",
        "url": "https://git.morosystems.cz/users/tomas.jilek/repos/moro-components-demo"
    },
    "publishConfig": {
        "registry": "https://nexus.morosystems.cz/content/repositories/npm-internal/"
    }
}
